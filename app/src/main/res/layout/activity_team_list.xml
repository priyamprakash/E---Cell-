<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/dash2"
    android:orientation="vertical"
    tools:context=".TeamList">




        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <Button
                android:id="@+id/button1"
                android:onClick="faculty"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="1dp"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                android:background="@drawable/side_cut_yellow"
                android:fontFamily="@font/poppins_light"
                android:text="Faculty mebers"
                android:layout_margin="20dp"
                android:textAllCaps="false"
                android:textColor="#000"
                android:textSize="16dp"
                android:textStyle="bold" />


            <Button
                android:id="@+id/button2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="1dp"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                android:background="@drawable/side_cut_yellow"
                android:fontFamily="@font/poppins_light"
                android:onClick="student"
                android:text="Student Coordinators"
                android:layout_margin="20dp"
                android:textAllCaps="false"
                android:textColor="#000"
                android:textSize="16dp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="1dp"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                android:background="@drawable/side_cut_yellow"
                android:fontFamily="@font/poppins_light"
                android:onClick="family"
                android:text="E-Cell Family"
                android:layout_margin="20dp"
                android:textAllCaps="false"
                android:textColor="#000"
                android:textSize="16dp"
                android:textStyle="bold" />
        </LinearLayout>

</LinearLayout>